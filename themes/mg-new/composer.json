{
	"name": "objectiv/objectiv-starter-theme",
	"description": "A starter theme for Objectiv websites",
	"type": "wordpress-theme",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"config": {
		"optimize-autoloader": true,
		"classmap-authoritative": true
	},
	"require": {
		"timber/timber": "^1.1",
		"mindkomm/timmy": "^0.14.0",
		"djboris88/timber-commented-include": "^1.0"
	},
	"require-dev": {
		"kint-php/kint": "^3.3",
		"kint-php/kint-twig": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Objectiv\\Site\\": "src/php"
		},
		"files": [
			"inc/tha-theme-hooks.php"
		]
	}
}
