{% block html_head_container %}
	{% include 'html-head.twig' %}
{% endblock %}
<body class="mg-body {{body_class}}" itemscope itemtype="http://schema.org/WebPage" data-template="base.twig">
	{{function('tha_body_top')}}

	<div
		class="site-wrap">
		<!-- Site Container -->
		<div
			class="site-container">

			<!-- Site Header -->
			{% block header %}
				{{function('tha_header_before')}}
				{% include('header.twig') %}
				{{function('tha_header_after')}}
			{% endblock %}

			<!-- Site Inner -->
			{% block layout %}
				Sorry, no content
			{% endblock %}

			<!-- Site Footer -->
			{% block footer %}
				{{function('tha_footer_before')}}
				{% include('footer.twig') %}
				{{function('tha_footer_after')}}
			{% endblock %}

		</div>

		{% include 'components/nav/mobile-nav.twig' %}
		{% include 'components/cart/sidebar-cart.twig' with {
				"woocommerce": woocommerce
			}
		%}
	</div>

	{{function('tha_body_bottom')}}
	{{function('wp_footer')}}
</body>
</html>
