<section class="fifty-fifty-front-page fifty-fifty">
    <div class="wrap">
	{% for fifty in fifty_fifty %}
		<div class="pane">
			<div class="content-wrap">
				<h3 class="title">{{ fifty['title'] }}</h3>
				<div class="text">{{ fifty['content'] }}</div>
				<a href="{{ fifty['link'] }}" class="button button__small" target="_blank">{{ fifty['link_text'] }}</a>
			</div>
		</div>
	{% endfor %}
    </div>
</section>
