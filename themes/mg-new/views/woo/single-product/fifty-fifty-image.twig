<section id="fifty-fifty-image" class="fifty-fifty-image wrap">

	{% for image in images %}
		<div class="fifty-fifty-image__body">
			{% if image['title'] is not empty %}
				<h3 class="fifty-fifty-image__title">{{image['title']}}</h3>
			{% endif %}
			
			<img src="{{image['image'] | get_timber_image_src('medium')}}" class="fifty-fifty-image__image"/>
		</div>
	{% endfor %}

</section>
